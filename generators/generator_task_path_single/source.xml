                    <string value="undefined"/>
                </equals>
            </conditions>
        </constraint>

        <constraint checkOnce="true" failMessage="The robot must be in the start area before launch!">
            <inside objectId="robot1" regionId="start"/>
        </constraint>

		<event settedUpInitially="true">
			<conditions glue = "and">
				<not>
					<inside objectId="robot1" regionId="finish"/>
				</not>
				<greater>
					<objectState object="robot1.display.labels.size"/>
					<int value="0"/>
				</greater>
				<equals>
					<objectState object="robot1.display.labels.first.text"/>
					<string value="finish"/>
				</equals>
			</conditions>
			<trigger>
				<fail message="Something is wrong!"/>
			</trigger>
		</event>

		<event settedUpInitially="true">
			<conditions glue = "and">
				<inside objectId="robot1" regionId="finish"/>
				<greater>
					<objectState object="robot1.display.labels.size"/>
					<int value="0"/>
				</greater>
				<equals>
					<objectState object="robot1.display.labels.first.text"/>
					<string value="finish"/>
				</equals>
			</conditions>
			<trigger>
				<success/>
			</trigger>
		</event>
